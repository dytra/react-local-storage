{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "es2017"
    ],
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
, "useLocalStorage.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
